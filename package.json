{
  "name": "openbridge-exporter",
  "version": "0.1.0",
  "description": "Exports svgs for openbridge",
  "main": "src/main.ts",
  "repository": {
    "url": "https://gitlab.com/openbridge/openbridge-exporter"
  },
  "scripts": {
    "start": "rimraf ./generated-* && npm-run-all --parallel autogen-*",
    "autogen-with-styles": "ts-node --files src/main.ts",
    "autogen-without-styles": "ts-node --files src/main.ts -c",
    "lint": "eslint . --ext .ts",
    "storybook": "start-storybook -p 6006",
    "build": "build-storybook -o public/",
    "dev": "webpack-dev-server --open --mode development"
  },
