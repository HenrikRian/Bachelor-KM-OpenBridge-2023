{
  "name": "openbridge-exporter",
  "version": "0.1.0",
  "description": "Exports svgs for openbridge",
  "main": "src/main.ts",
  "repository": {
    "url": "https://gitlab.com/openbridge/openbridge-exporter"
  },
  "scripts": {
    "start": "rimraf ./generated-* && npm run autogen-with-styles && npm run autogen-without-styles",
    "autogen-with-styles": "ts-node --files src/main.ts",
    "autogen-without-styles": "ts-node --files src/main.ts -c",
    "start-vue": "rimraf ./gen-vue && ts-node --files src/main-vue.ts",
    "lint": "eslint . --ext .ts",
    "storybook": "start-storybook -p 6006",
    "build": "build-storybook -o public/",
    "prepare": "npm run start && npm run start-vue"
  },
  "author": "",
  "license": "SEE LICENSE IN license.txt",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@storybook/addon-actions": "^6.0.19",
    "@storybook/addon-essentials": "^6.0.19",
    "@storybook/addon-links": "^6.0.19",
    "@storybook/vue": "^6.0.19",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/xmldom": "^0.1.30",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "fast-xml-parser": "^3.17.4",
    "fs": "0.0.1-security",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "openbridge-components": "git+https://gitlab.com/openbridge/openbridge-components.git#58e99b76746e2265dbad1cdb732404db94b85c6d",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.3",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.12",
    "xmldom": "^0.3.0",
    "yargs": "^16.0.3"
  }
}
