    "@storybook/addon-links": "^6.0.22",
    "@storybook/addons": "^6.0.22",
    "@storybook/react": "^6.0.22",
    "@storybook/source-loader": "^6.0.22",
    "@types/node": "^14.6.0",
    "@types/node-fetch": "^2.5.7",
    "@types/xmldom": "^0.1.30",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
